{
  "env": {
    "es2021": true,
    "node": true
  },
  "extends": "airbnb-base",
  "overrides": [],
  "rules": {
    "import/no-extraneous-dependencies": ["error", {"devDependencies": true}],
    "quotes": ["error", "single"],
    "no-underscore-dangle": ["error", {
        "allow": [
          "_id"
        ]
      }
    ]
  ,
"eol-last": 0,
"linebreak-style":0
}
}
